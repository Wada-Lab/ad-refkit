module oled_i2c_mon_tb();

    localparam FREQ = 200_000_000;

    oled_i2c_mon_tb#(.CLK_DIV(FREQ/1000), .FREQ_MHZ(FREQ/1000/1000))
    DUT(.clk(clk),
	.reset(reset),
	.dbg_tl(dbg_tl),
	.dbg_tr(dbg_tr),
	.dbg_bl(dbg_bl),
	.dbg_br(dbg_br),
	.sda_t(sda_t),
	.sda_o(sda_o),
	.sda_i(sda_i),
	.scl_t(scl_t),
	.scl_o(scl_o),
	.scl_i(scl_i)
     );
